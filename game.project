[project]
title = FMOD
version = 0.1
custom_resources = example/banks/

[bootstrap]
main_collection = /example/example.collectionc

[input]
game_binding = /example/game.input_bindingc
use_accelerometer = 0

[display]
width = 1280
height = 720
high_dpi = 1

[physics]
scale = 0.02

[script]
shared_state = 1

[library]
include_dirs = fmod

[fmod]
lib_path = fmod/res
speaker_mode = 5.1

